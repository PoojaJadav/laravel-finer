{
  "name": "rjchauhan/laravel-finer",
  "description": "Laravel multi-purpose helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Raviraj Chauhan",
      "email": "rjchauhan8427@gmail.com",
      "homepage": "https://github.com/rjchauhan"
    }
  ],
  "homepage": "https://github.com/rjchauhan/laravel-finer",
  "keywords": [
    "laravel",
    "laravel finer",
    "rjchauhan"
  ],
  "require": {
    "php": "^7.2|^8.1",
    "barryvdh/laravel-dompdf": "^1.0",
    "illuminate/support": "~5|~6|~7|~8|~9",
    "nesbot/carbon": "^2.26",
    "spatie/laravel-activitylog": "^3.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "~3|~4|~5|~6",
    "sempro/phpunit-pretty-print": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Rjchauhan\\LaravelFiner\\": "src/"
    },
    "files": [
      "utils/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rjchauhan\\LaravelFiner\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rjchauhan\\LaravelFiner\\LaravelFinerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
